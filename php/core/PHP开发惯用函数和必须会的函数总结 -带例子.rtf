<?php\par
public function request_by_curl($remote_server, $post_array)\par
\{  \par
$ch = curl_init();        \par
curl_setopt($ch, CURLOPT_HEADER,0);\par
curl_setopt($ch, CURLOPT_RETURNTRANSFER, 1);\par
curl_setopt($ch, CURLOPT_URL, $remote_server);\par
curl_setopt($ch, CURLOPT_POST, 1);\par
curl_setopt($ch, CURLOPT_POSTFIELDS, $post_array);     \par
$result = curl_exec($ch);   \par
if (curl_errno($ch)) \{\par
   echo 'Errno'.curl_error($ch);//\'b2\'b6\'d7\'a5\'d2\'ec\'b3\'a3\par
   exit;\par
   //print_r(curl_getinfo($ch));\par
\}\par
curl_close($ch);\par
return $result;\par
\}\par
?>\par
